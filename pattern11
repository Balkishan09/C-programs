#include<iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    int i=1;
    
    while(i<=n){
        
        int j=1;
        
        while(j<=i){
            cout<<i-j+1;  
           
                   
            j++;
        }
        
        cout<<endl;
        i++;
    }
}

/*ANOTHER METHOD
#include<iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    int i=1;
    
    while(i<=n){
        int start=i;
        int j=1;
        
        while(j<=i){
            cout<<start;  
            start--;
                   
            j++;
        }
        
        cout<<endl;
        i++;
    }
}


